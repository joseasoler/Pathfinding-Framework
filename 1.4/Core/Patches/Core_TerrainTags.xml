<?xml version="1.0" encoding="utf-8"?>
<!--
Ensure that terrains to be patched by PF have a tags node.
-->
<Patch>

	<Operation Class="PatchOperationConditional">
		<xpath>Defs/TerrainDef[defName="Gravel"]/tags
		</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/TerrainDef[defName="Gravel"]
			</xpath>
			<value>
				<tags/>
			</value>
		</nomatch>
	</Operation>

