<?xml version="1.0" encoding="utf-8"?>
<!-- ToDo delete this file after the initial testing phase. -->
<Patch>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/LifeStageDef[defName="HumanlikePreTeenager"]</xpath>
		<value>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Aquatic</movementDef>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Apparel_CowboyHat" or defName="Warg"]</xpath>
		<value>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Aquatic</movementDef>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Megasloth"]</xpath>
		<value>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Flying</movementDef>
			</li>
		</value>
	</Operation>

	<!-- Flying pigskins. -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/GeneDef[defName="VoicePig"]</xpath>
		<value>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Flying</movementDef>
			</li>
		</value>
	</Operation>

	<!-- Amphibious impids. -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/GeneDef[defName="FireSpew"]</xpath>
		<value>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Amphibious</movementDef>
			</li>
		</value>
	</Operation>

</Patch>